{
    "name": "rsubr/nova-json-schema-field",
    "description": "A Laravel Nova field for JSON Schema.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Rsubr\\NovaJsonSchemaField\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rsubr\\NovaJsonSchemaField\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
